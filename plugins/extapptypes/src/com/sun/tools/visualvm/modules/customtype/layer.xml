<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.1//EN" "http://www.netbeans.org/dtds/filesystem-1_1.dtd">
<filesystem>
    <folder name="VisualVM">
         <folder name="ExplorerPopupSelection">
            <file name="com-sun-tools-visualvm-modules-customtype-actions-NewApplicationTypeAction.instance">
                <attr name="instanceCreate" methodvalue="com.sun.tools.visualvm.modules.customtype.actions.NewApplicationTypeAction.getDefault"/>
                <attr name="position" intvalue="3000"/>
            </file>
            <file name="com-sun-tools-visualvm-modules-customtype-actions-EditApplicationTypeAction.instance">
                <attr name="instanceCreate" methodvalue="com.sun.tools.visualvm.modules.customtype.actions.EditApplicationTypeAction.getDefault"/>
                <attr name="position" intvalue="3100"/>
            </file>
        </folder>
        <folder name="ApplicationTypes">
            <!--
                Here come the registration of the custom application types
                They must be of the following form

                <folder name="Application.def"> // *.def subfolders are treated as apptype definitions
                    <attr name="displayName" stringvalue="<Custom Application Name>"/>
                    <attr name="icon" stringvalue="<URL to Icon; can use special NB protocols like nbres:/ or nbfs:/ >"/>
                    <attr name="mainClass" stringvalue="<main class identifier>"/>
                    <attr name="url" stringvalue="URL for more information"/>
                    <attr name="description" stringvalue="description"/>
                </folder>
            -->
        </folder>
    </folder>
    <folder name="OptionsDialog">
        <file name="com-sun-tools-visualvm-modules-customtype-options-ApplicationTypeOptionsCategory.instance">
            <attr name="instanceCreate" methodvalue="com.sun.tools.visualvm.modules.customtype.options.ApplicationTypeOptionsCategory.instance"/>
            <attr name="position" intvalue="5000"/>
        </file>
    </folder>
</filesystem>
